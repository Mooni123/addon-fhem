{
  "name": "FHEM",
  "version": "0.0.2",
  "slug": "fhem",
  "description": "FHEM Plugin",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "url": "https://github.com/Mooni123/addon-fhem",
  "startup": "services",
  "boot": "auto",
  "webui": "http://[HOST]:[PORT:8083]",
  "panel_icon": "mdi:HomeFloorL",
  "panel_title": "FHEM",
  "uart": true,
  "gpio": false,
  "ingress": true,
  "ports": {
    "8083/tcp": 8083
  },
  "ports_description": {
    "8083/tcp": "WebUI"
  },
  "map": ["config:rw"],
  "host_network": false,
  "environment": {
  },
  "devices": ["/dev/serial/by-id/usb-1a86_USB2.0-Serial-if00-port0"],
  "privileged": [
    "SYS_ADMIN",
    "SYS_RAWIO"
  ],
  "init": false,
  "options": {
    "apt_pks": "",
    "pip_pks": "",
    "npm_pks": "",
    "cpan_pks": "Net::MQTT  Net::MQTT::Message Net::MQTT::Message::JustMessageId",
    "device": "/dev/serial/by-id/usb-1a86_USB2.0-Serial_-if00-port0",
    "baud": "57600"
  },
  "schema": {
    "apt_pks": "str",
    "pip_pks": "str",
    "npm_pks": "str",
    "cpan_pks": "str",
    "device": "device(subsystem=tty)",
    "baud": ["9600","19200","38400","57600","115200"]
  }

}
